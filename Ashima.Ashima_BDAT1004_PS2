    "def binary(n):\n",
    "    if n > 1:\n",
    "        binary(n // 2)  # Recursive call with integer division\n",
    "    print(n % 2, end='')\n",
    "\n",
    "# Test the method\n",
    "num = 45\n",
    "print(f\"The binary representation of {num} is: \", end='')\n",
    "binary(num)"
   ]
